---
import "../assets/css/styles.css";
import Header from "../components/Header.astro";
import Layout from "../layouts/Layout.astro";
import MyInformation from "../components/MyInformation.astro";

import data from "../config/data.json";
---

<Layout>
  <Header
    name={data.header.name}
    position={data.header.position}
    company={data.header.company}
  />

  <MyInformation
    description={data.my_information.description}
    technologies={data.my_information.technologies}
    socialNetworks={data.my_information.social_networks}
    projects={6}
    yearsOfExperience={data.my_information.years_of_experience}
    talksAndWorkshops={6}
  />
</Layout>
